<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <imports>
        <import resource="admin/attribute_group.xml"/>
    </imports>

    <services>
        <service id="attribute_group.manager" class="WellCommerce\Bundle\AttributeBundle\Manager\AttributeGroupManager" parent="core.manager">
            <argument type="service" id="attribute_group.repository"/>
            <argument type="service" id="attribute_group.factory"/>
        </service>
    </services>
</container>
